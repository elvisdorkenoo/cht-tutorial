        <data id="contact:health_facility:edit" prefix="J1!contact:health_facility:edit!" delimiter="#" version="2022-07-04 21-04">
          <inputs>
            <meta>
              <location>
                <lat/>
                <long/>
                <error/>
                <message/>
              </location>
            </meta>
            <user>
              <contact_id/>
              <facility_id/>
              <name/>
            </user>
          </inputs>
          <health_facility>
            <parent/>
            <type/>
            <contact>
              <name/>
              <contact_name/>
              <_id/>
            </contact>
            <is_name_generated/>
            <generated_name/>
            <name/>
            <external_id/>
            <address/>
            <notes/>
            <geolocation/>
            <meta tag="hidden">
              <created_by/>
              <created_by_person_uuid/>
              <created_by_place_uuid/>
              <last_edited_by/>
              <last_edited_by_person_uuid/>
              <last_edited_by_place_uuid/>
            </meta>
          </health_facility>
          <init>
            <place_type/>
            <place_type_translation/>
            <select_generated_name/>
            <generated_name_translation/>
