      <bind nodeset="/mother_referral_followup/facility_id" type="string" calculate="../inputs/contact/parent/parent/parent/_id"/>
      <bind nodeset="/mother_referral_followup/patient_age_in_years" type="string" calculate="floor( difference-in-months( ../inputs/contact/date_of_birth, today() ) div 12 )"/>
      <bind nodeset="/mother_referral_followup/patient_uuid" type="string" calculate="../inputs/contact/_id"/>
      <bind nodeset="/mother_referral_followup/patient_id" type="string" calculate="../inputs/contact/patient_id"/>
      <bind nodeset="/mother_referral_followup/patient_name" type="string" calculate="../inputs/contact/name"/>
      <bind nodeset="/mother_referral_followup/patient_short_name" type="string" calculate="coalesce(../inputs/contact/short_name, ../summary/custom_translations/custom_woman_label)"/>
      <bind nodeset="/mother_referral_followup/patient_short_name_start" type="string" calculate="coalesce(../inputs/contact/short_name, ../summary/custom_translations/custom_woman_start_label)"/>
      <bind nodeset="/mother_referral_followup/followup_count" type="string" calculate="../inputs/t_follow_up_count"/>
      <bind nodeset="/mother_referral_followup/follow_up_count_number" type="string" calculate="number( /mother_referral_followup/inputs/t_follow_up_count )"/>
      <bind nodeset="/mother_referral_followup/delivery_date" type="string" calculate="../inputs/t_delivery_date"/>
      <bind nodeset="/mother_referral_followup/days_since_delivery" type="string" calculate="floor( decimal-date-time(today()) - decimal-date-time( /mother_referral_followup/delivery_date ) )"/>
      <bind nodeset="/mother_referral_followup/months_since_delivery" type="string" calculate="difference-in-months( /mother_referral_followup/delivery_date , today())"/>
      <bind nodeset="/mother_referral_followup/weeks_since_delivery" type="string" calculate="round( /mother_referral_followup/delivery_date  div 7, 0)"/>
      <bind nodeset="/mother_referral_followup/refer_cscom" type="string" calculate="if( /mother_referral_followup/g_followup/g_disease_progress/health_status  = 'aggravated' or  /mother_referral_followup/g_followup/g_disease_progress/health_status  = 'unchanged' ,'true','false')"/>
      <bind nodeset="/mother_referral_followup/continue_followup" type="string" calculate="if( /mother_referral_followup/g_task/task_to_perform ='referral_followup' and  /mother_referral_followup/g_followup/g_disease_progress/health_status  != 'improved' and  /mother_referral_followup/g_followup/g_disease_progress/health_status  != 'healed' ,'true','false')"/>
      <bind nodeset="/mother_referral_followup/next_followup_days" type="string" calculate="format-date-time(date-time(decimal-date-time(today())+ 1),&quot;%d/%m/%Y&quot;)"/>
      <bind nodeset="/mother_referral_followup/g_task/task_to_perform" type="select1" jr:requiredMsg="jr:itext('/mother_referral_followup/g_task/task_to_perform:jr:requiredMsg')" required="true()"/>
      <bind nodeset="/mother_referral_followup/g_followup" relevant=" /mother_referral_followup/g_task/task_to_perform ='referral_followup'"/>
