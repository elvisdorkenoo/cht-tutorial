      <bind nodeset="/data/inputs/user/contact_id" type="string"/>
      <bind nodeset="/data/inputs/user/facility_id" type="string"/>
      <bind nodeset="/data/inputs/user/name" type="string"/>
      <bind nodeset="/data/init/place_type" type="select1" calculate="&quot;c80_concession&quot;"/>
      <bind nodeset="/data/init/place_name_selector" type="select1"/>
      <bind nodeset="/data/init/place_name_label" type="string" calculate="jr:choice-name( /data/init/place_name_selector ,' /data/init/place_name_selector ')"/>
      <bind nodeset="/data/init/user_contact/contact_uuid" type="string" readonly="true()" calculate=" /data/inputs/user/contact_id "/>
      <bind nodeset="/data/init/user_contact/role" type="string"/>
      <bind nodeset="/data/init/user_contact/user_role" type="string" calculate="../role"/>
      <bind nodeset="/data/init/user_contact/n_edit" readonly="true()" type="string" relevant=" /data/init/user_contact/user_role  = 'oca'"/>
      <bind nodeset="/data/c80_concession/parent" type="string" readonly="true()"/>
      <bind nodeset="/data/c80_concession/type" type="string" readonly="true()"/>
      <bind nodeset="/data/c80_concession/contact_name_note" readonly="true()" type="string"/>
      <bind nodeset="/data/c80_concession/change_primary" type="select1" required="true()"/>
      <bind nodeset="/data/c80_concession/contact/name" type="string" readonly="true()"/>
      <bind nodeset="/data/c80_concession/contact/contact_name" type="string" calculate="../name"/>
      <bind nodeset="/data/c80_concession/contact/select_c12" type="string" relevant=" /data/c80_concession/change_primary  = 'true' and  /data/init/place_type  = 'c10_central'" required="true()"/>
      <bind nodeset="/data/c80_concession/contact/select_c72" type="string" relevant=" /data/c80_concession/change_primary  = 'true' and  /data/init/place_type  = 'c70_village'" required="true()"/>
      <bind nodeset="/data/c80_concession/contact/select_c82" type="string" relevant=" /data/c80_concession/change_primary  = 'true' and  /data/init/place_type  = 'c80_concession'" required="true()"/>
      <bind nodeset="/data/c80_concession/contact/select_c92" type="string" relevant=" /data/c80_concession/change_primary  = 'true' and  /data/init/place_type  = 'c90_household'" required="true()"/>
      <bind nodeset="/data/c80_concession/contact/_id" type="string" calculate="if( /data/c80_concession/change_primary  = 'true',
coalesce(
../select_c12,
../select_c22,
../select_c32,
../select_c42,
../select_c52,
../select_c62,
../select_c72,
../select_c82,
../select_c92
), .)"/>
      <bind nodeset="/data/c80_concession/name" type="string" constraint=" /data/init/user_contact/user_role  != 'oca'" required="true()" jr:constraintMsg="jr:itext('/data/c80_concession/name:jr:constraintMsg')"/>
      <bind nodeset="/data/c80_concession/geolocation" type="string" calculate="concat(../../inputs/meta/location/lat, concat(' ', ../../inputs/meta/location/long))"/>
      <bind nodeset="/data/c80_concession/family_number" type="int" relevant=" /data/init/place_type  = 'c80_concession'" jr:constraintMsg="jr:itext('/data/c80_concession/family_number:jr:constraintMsg')" required="true()" constraint=". &gt;= 1 and . &lt;= 50"/>
      <bind nodeset="/data/c80_concession/family_geolocation" type="geopoint" relevant=" /data/init/place_type  = 'c80_concession'"/>
      <bind nodeset="/data/c80_concession/meta/created_by" type="string" readonly="true()"/>
      <bind nodeset="/data/c80_concession/meta/created_by_person_uuid" type="string" readonly="true()"/>
      <bind nodeset="/data/c80_concession/meta/created_by_place_uuid" type="string"/>
      <bind nodeset="/data/c80_concession/meta/last_edited_by" type="string" calculate="../../../inputs/user/name"/>
      <bind nodeset="/data/c80_concession/meta/last_edited_by_person_uuid" type="string" calculate="../../../inputs/user/contact_id"/>
      <bind nodeset="/data/c80_concession/meta/last_edited_by_place_uuid" type="string" calculate="../../../inputs/user/facility_id"/>
      <bind nodeset="/data/meta/instanceID" type="string" readonly="true()" calculate="concat('uuid:', uuid())"/>
    </model>
