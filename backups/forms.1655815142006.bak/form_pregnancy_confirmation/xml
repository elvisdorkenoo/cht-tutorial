      <bind nodeset="/pregnancy_confirmation/patient_age_in_months" type="string" calculate="difference-in-months(  /pregnancy_confirmation/inputs/contact/date_of_birth , today() )"/>
      <bind nodeset="/pregnancy_confirmation/patient_uuid" type="string" calculate="../inputs/contact/_id"/>
      <bind nodeset="/pregnancy_confirmation/patient_id" type="string" calculate="../inputs/contact/patient_id"/>
      <bind nodeset="/pregnancy_confirmation/patient_name" type="string" calculate="../inputs/contact/name"/>
      <bind nodeset="/pregnancy_confirmation/patient_age_display_fr" type="string" calculate="concat(concat( /pregnancy_confirmation/patient_age_in_years , if( /pregnancy_confirmation/patient_age_in_years =1,' an et ',' ans et ')),concat( /pregnancy_confirmation/patient_age_in_months  mod 12, if(( /pregnancy_confirmation/patient_age_in_months  mod 12)=1,' mois',' mois')))"/>
      <bind nodeset="/pregnancy_confirmation/patient_age_display" type="string" calculate="concat(concat( /pregnancy_confirmation/patient_age_in_years , if( /pregnancy_confirmation/patient_age_in_years =1,' year and ',' years and ')),concat( /pregnancy_confirmation/patient_age_in_months  mod 12, if(( /pregnancy_confirmation/patient_age_in_months  mod 12)=1,' month old',' months old')))"/>
