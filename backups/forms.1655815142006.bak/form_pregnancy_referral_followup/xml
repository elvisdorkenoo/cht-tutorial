      <bind nodeset="/pregnancy_referral_followup/household_id" type="string" calculate="../inputs/contact/parent/_id"/>
      <bind nodeset="/pregnancy_referral_followup/area_id" type="string" calculate="../inputs/contact/parent/parent/_id"/>
      <bind nodeset="/pregnancy_referral_followup/facility_id" type="string" calculate="../inputs/contact/parent/parent/parent/_id"/>
      <bind nodeset="/pregnancy_referral_followup/patient_age_in_years" type="string" calculate="floor( difference-in-months( ../inputs/contact/date_of_birth, today() ) div 12 )"/>
      <bind nodeset="/pregnancy_referral_followup/patient_uuid" type="string" calculate="../inputs/contact/_id"/>
      <bind nodeset="/pregnancy_referral_followup/patient_id" type="string" calculate="../inputs/contact/patient_id"/>
      <bind nodeset="/pregnancy_referral_followup/patient_name" type="string" calculate="../inputs/contact/name"/>
      <bind nodeset="/pregnancy_referral_followup/patient_short_name" type="string" calculate="coalesce(../inputs/contact/short_name, ../summary/custom_translations/custom_woman_label)"/>
      <bind nodeset="/pregnancy_referral_followup/patient_short_name_start" type="string" calculate="coalesce(../inputs/contact/short_name, ../summary/custom_translations/custom_woman_start_label)"/>
      <bind nodeset="/pregnancy_referral_followup/lmp_date" type="string" calculate=" /pregnancy_referral_followup/inputs/t_lmp_date "/>
      <bind nodeset="/pregnancy_referral_followup/days_since_lmp" type="string" calculate="round(decimal-date-time(today()) - decimal-date-time( /pregnancy_referral_followup/lmp_date ), 2)"/>
      <bind nodeset="/pregnancy_referral_followup/weeks_since_lmp" type="string" calculate="round( /pregnancy_referral_followup/days_since_lmp  div 7, 0)"/>
      <bind nodeset="/pregnancy_referral_followup/remainder_days_since_lmp" type="string" calculate="round( /pregnancy_referral_followup/days_since_lmp  mod 7,0)"/>
      <bind nodeset="/pregnancy_referral_followup/months_since_lmp" type="string" calculate="round( /pregnancy_referral_followup/days_since_lmp  mod 30,0)"/>
      <bind nodeset="/pregnancy_referral_followup/estimated_delivey_date" type="string" calculate="format-date-time(date-time(decimal-date-time( /pregnancy_referral_followup/lmp_date )+280),&quot;%Y-%m-%d&quot;)"/>
