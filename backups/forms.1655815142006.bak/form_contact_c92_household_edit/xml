      <bind nodeset="/data/inputs/user/name" type="string"/>
      <bind nodeset="/data/init/user_contact/contact_uuid" type="string" readonly="true()" calculate=" /data/inputs/user/contact_id "/>
      <bind nodeset="/data/init/user_contact/role" type="string"/>
      <bind nodeset="/data/init/user_contact/user_role" type="string" calculate="../role"/>
      <bind nodeset="/data/init/user_contact/n_edit" readonly="true()" type="string" relevant=" /data/init/user_contact/user_role  = 'oca'"/>
      <bind nodeset="/data/c92_household/parent" type="string"/>
      <bind nodeset="/data/c92_household/first_name" type="string" required="true()"/>
      <bind nodeset="/data/c92_household/last_name" type="string" required="true()"/>
      <bind nodeset="/data/c92_household/name" type="string" calculate="concat( /data/c92_household/first_name ,' ', /data/c92_household/last_name )"/>
      <bind nodeset="/data/c92_household/date_of_birth" type="string" calculate="../ephemeral_dob/dob_iso" required="true()"/>
      <bind nodeset="/data/c92_household/date_of_birth_method" type="string" calculate="../ephemeral_dob/dob_method"/>
      <bind nodeset="/data/c92_household/person_age_in_years" type="string" calculate="floor(difference-in-months( /data/c92_household/date_of_birth , today() ) div 12 )"/>
      <bind nodeset="/data/c92_household/person_age_in_months" type="string" calculate="difference-in-months(  /data/c92_household/date_of_birth , today() )"/>
      <bind nodeset="/data/c92_household/person_age_in_days" type="string" calculate="floor( decimal-date-time(today()) - decimal-date-time( /data/c92_household/date_of_birth ) )"/>
      <bind nodeset="/data/c92_household/ephemeral_dob/dob_calendar" type="date" jr:constraintMsg="jr:itext('/data/c92_household/ephemeral_dob/dob_calendar:jr:constraintMsg')" constraint="floor(decimal-date-time(.)) &lt;= floor(decimal-date-time(today()))" relevant="not(selected(../dob_method,'approx'))" required="true()"/>
      <bind nodeset="/data/c92_household/ephemeral_dob/age_label" readonly="true()" type="string" relevant="selected(../dob_method,'approx')"/>
      <bind nodeset="/data/c92_household/ephemeral_dob/age_years" type="int" jr:constraintMsg="jr:itext('/data/c92_household/ephemeral_dob/age_years:jr:constraintMsg')" constraint=". &gt;= 0 and . &lt;= 130" relevant="selected(../dob_method,'approx')" required="true()"/>
      <bind nodeset="/data/c92_household/ephemeral_dob/age_months" type="int" jr:constraintMsg="jr:itext('/data/c92_household/ephemeral_dob/age_months:jr:constraintMsg')" constraint=". &gt;= 0 and . &lt;= 11" relevant="selected(../dob_method,'approx')"/>
      <bind nodeset="/data/c92_household/ephemeral_dob/dob_method" type="select"/>
      <bind nodeset="/data/c92_household/ephemeral_dob/ephemeral_months" type="string" calculate="if(format-date-time(today(),&quot;%m&quot;) - ../age_months &lt; 0, format-date-time(today(),&quot;%m&quot;) - ../age_months + 12, format-date-time(today(),&quot;%m&quot;) - ../age_months)"/>
      <bind nodeset="/data/c92_household/ephemeral_dob/ephemeral_years" type="string" calculate="if(format-date-time(today(),&quot;%m&quot;) - ../age_months &lt; 0, format-date-time(today(),&quot;%Y&quot;) - ../age_years - 1, format-date-time(today(),&quot;%Y&quot;) -../age_years)"/>
      <bind nodeset="/data/c92_household/ephemeral_dob/dob_approx" type="string" calculate="concat(string( /data/c92_household/ephemeral_dob/ephemeral_years ),'-',if( /data/c92_household/ephemeral_dob/ephemeral_months &lt;10, concat('0',string( /data/c92_household/ephemeral_dob/ephemeral_months )),  /data/c92_household/ephemeral_dob/ephemeral_months ),'-',string(format-date-time(today(), &quot;%d&quot;)))"/>
      <bind nodeset="/data/c92_household/ephemeral_dob/dob_raw" type="string" calculate="if(not(selected( ../dob_method,'approx')), 
../dob_calendar,
../dob_approx)"/>
      <bind nodeset="/data/c92_household/ephemeral_dob/dob_iso" type="string" calculate="format-date-time(../dob_raw,&quot;%Y-%m-%d&quot;)"/>
      <bind nodeset="/data/c92_household/ephemeral_dob/dob_debug" readonly="true()" type="string" relevant="false()"/>
      <bind nodeset="/data/c92_household/sex" type="select1" jr:constraintMsg="jr:itext('/data/c92_household/sex:jr:constraintMsg')" constraint=" /data/init/user_contact/user_role  != 'oca'" required="true()"/>
      <bind nodeset="/data/c92_household/phone" type="string" jr:constraintMsg="jr:itext('/data/c92_household/phone:jr:constraintMsg')" constraint="regex(.,'^(\d{2}\s?){4}$')" required="true()"/>
