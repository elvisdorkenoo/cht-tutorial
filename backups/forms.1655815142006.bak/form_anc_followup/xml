      <bind nodeset="/anc_followup/patient_age_in_years" type="string" calculate="floor( difference-in-months(  /anc_followup/inputs/contact/date_of_birth , today() ) div 12 )"/>
      <bind nodeset="/anc_followup/patient_age_in_months" type="string" calculate="difference-in-months(  /anc_followup/inputs/contact/date_of_birth , today() )"/>
      <bind nodeset="/anc_followup/patient_age_in_days" type="string" calculate="floor( decimal-date-time(today()) - decimal-date-time( /anc_followup/inputs/contact/date_of_birth ) )"/>
      <bind nodeset="/anc_followup/patient_age_display" type="string" calculate="concat(concat( /anc_followup/patient_age_in_years , if( /anc_followup/patient_age_in_years =1,' year and ',' years and ')),concat( /anc_followup/patient_age_in_months  mod 12, if(( /anc_followup/patient_age_in_months  mod 12)=1,' month old',' months old')))"/>
      <bind nodeset="/anc_followup/patient_age_display_fr" type="string" calculate="concat(concat( /anc_followup/patient_age_in_years , if( /anc_followup/patient_age_in_years =1,' an et ',' ans et ')),concat( /anc_followup/patient_age_in_months  mod 12, if(( /anc_followup/patient_age_in_months  mod 12)=1,' mois',' mois')))"/>
      <bind nodeset="/anc_followup/patient_age_display_bm" type="string" calculate="concat( concat( if( /anc_followup/patient_age_in_years  != '','San ',''),  /anc_followup/patient_age_in_years  ), concat( if(( /anc_followup/patient_age_in_months  mod 12)=1,' ani kalo ',' ani kalo ')  /anc_followup/patient_age_in_months  mod 12, ) )"/>
      <bind nodeset="/anc_followup/patient_sex_bm" type="string" calculate="if( /anc_followup/patient_sex  = 'male' or  /anc_followup/patient_sex  = 'homme' or  /anc_followup/patient_sex  = 'Homme', 'Cɛ', if( /anc_followup/patient_sex  = 'female' or  /anc_followup/patient_sex  = 'femme' or  /anc_followup/patient_sex  = 'Femme', 'Muso', ''))"/>
      <bind nodeset="/anc_followup/patient_id" type="string" required="true()" jr:requiredMsg="jr:itext('/anc_followup/patient_id:jr:requiredMsg')" calculate="../inputs/contact/_id"/>
      <bind nodeset="/anc_followup/patient_name" type="string" calculate="../inputs/contact/name"/>
      <bind nodeset="/anc_followup/patient_sex" type="string" calculate="../inputs/contact/sex"/>
      <bind nodeset="/anc_followup/patient_sex_en" type="string" calculate="if( /anc_followup/patient_sex  = 'male', 'Male', if( /anc_followup/patient_sex  = 'female', 'Female', ''))"/>
      <bind nodeset="/anc_followup/patient_sex_fr" type="string" calculate="if( /anc_followup/patient_sex  = 'male', 'Homme', if( /anc_followup/patient_sex  = 'female', 'Femme', ''))"/>
      <bind nodeset="/anc_followup/g_task/task_to_perform" type="select1" required="true()" jr:requiredMsg="jr:itext('/anc_followup/g_task/task_to_perform:jr:requiredMsg')"/>
      <bind nodeset="/anc_followup/g_realisation" relevant=" /anc_followup/g_task/task_to_perform ='reference_followup'"/>
      <bind nodeset="/anc_followup/g_realisation/followup_done_today" type="select1" required="true()" jr:requiredMsg="jr:itext('/anc_followup/g_realisation/followup_done_today:jr:requiredMsg')"/>
      <bind nodeset="/anc_followup/g_realisation/followup_date" type="date" constraint=". &lt;= today()-1" required="true()" jr:requiredMsg="jr:itext('/anc_followup/g_realisation/followup_date:jr:requiredMsg')" relevant=" /anc_followup/g_realisation/followup_done_today ='no'"/>
      <bind nodeset="/anc_followup/g_realisation/not_done_today" type="select1" required="true()" jr:requiredMsg="jr:itext('/anc_followup/g_realisation/not_done_today:jr:requiredMsg')" relevant=" /anc_followup/g_realisation/followup_done_today ='no'"/>
      <bind nodeset="/anc_followup/g_realisation/other_followup_reason" type="string" required="true()" jr:requiredMsg="jr:itext('/anc_followup/g_realisation/other_followup_reason:jr:requiredMsg')" relevant=" /anc_followup/g_realisation/not_done_today ='other'"/>
      <bind nodeset="/anc_followup/g_realisation/has_woman_been_found" type="select1" required="true()" jr:requiredMsg="jr:itext('/anc_followup/g_realisation/has_woman_been_found:jr:requiredMsg')"/>
      <bind nodeset="/anc_followup/g_realisation/how_patient_found" type="select1" required="true()" jr:requiredMsg="jr:itext('/anc_followup/g_realisation/how_patient_found:jr:requiredMsg')" relevant=" /anc_followup/g_realisation/has_woman_been_found ='yes'"/>
      <bind nodeset="/anc_followup/g_realisation/other_patient_found_reason" type="string" required="true()" jr:requiredMsg="jr:itext('/anc_followup/g_realisation/other_patient_found_reason:jr:requiredMsg')" relevant=" /anc_followup/g_realisation/how_patient_found ='other'"/>
      <bind nodeset="/anc_followup/g_anc_realization" relevant=" /anc_followup/g_realisation/has_woman_been_found ='yes'"/>
      <bind nodeset="/anc_followup/g_anc_realization/did_patient_go_for_anc" type="select1" required="true()" jr:requiredMsg="jr:itext('/anc_followup/g_anc_realization/did_patient_go_for_anc:jr:requiredMsg')"/>
      <bind nodeset="/anc_followup/g_anc_realization/when_was_anc_scheduled" type="date" required="false()" relevant=" /anc_followup/g_anc_realization/did_patient_go_for_anc ='yes'" jr:constraintMsg="jr:itext('/anc_followup/g_anc_realization/when_was_anc_scheduled:jr:constraintMsg')" constraint=". &lt;= today()-1"/>
      <bind nodeset="/anc_followup/g_anc_realization/when_was_anc_performed" type="date" required="false()" relevant=" /anc_followup/g_anc_realization/did_patient_go_for_anc ='yes'" jr:constraintMsg="jr:itext('/anc_followup/g_anc_realization/when_was_anc_performed:jr:constraintMsg')" constraint=". &lt; today()-1 and floor(decimal-date-time(.)) &gt;= floor(decimal-date-time( /anc_followup/g_anc_realization/when_was_anc_scheduled ))"/>
      <bind nodeset="/anc_followup/g_anc_realization/diff_date_anc" type="string" calculate="floor( decimal-date-time( /anc_followup/g_anc_realization/when_was_anc_performed ) - decimal-date-time( /anc_followup/g_anc_realization/when_was_anc_scheduled ))"/>
      <bind nodeset="/anc_followup/g_anc_realization/why_patient_missed_appointment" type="select1" required="true()" jr:requiredMsg="jr:itext('/anc_followup/g_anc_realization/why_patient_missed_appointment:jr:requiredMsg')" relevant=" /anc_followup/g_anc_realization/diff_date_anc  &gt; 0 or  /anc_followup/g_anc_realization/did_patient_go_for_anc ='no'"/>
      <bind nodeset="/anc_followup/g_anc_realization/specify_other" type="string" required="true()" jr:requiredMsg="jr:itext('/anc_followup/g_anc_realization/specify_other:jr:requiredMsg')" relevant=" /anc_followup/g_anc_realization/why_patient_missed_appointment ='other'"/>
      <bind nodeset="/anc_followup/g_next_anc_visit/next_anc_visit_date" type="date" required="false()" constraint=". &gt; today()" relevant=" /anc_followup/g_anc_realization/did_patient_go_for_anc ='no'"/>
      <bind nodeset="/anc_followup/g_next_anc_visit/is_new_anc_scheculed" type="select1" required="true()" jr:requiredMsg="jr:itext('/anc_followup/g_next_anc_visit/is_new_anc_scheculed:jr:requiredMsg')" relevant=" /anc_followup/g_anc_realization/did_patient_go_for_anc ='yes'"/>
      <bind nodeset="/anc_followup/g_next_anc_visit/anc_chosen_date" type="date" constraint=". &gt; today()" required="true()" jr:requiredMsg="jr:itext('/anc_followup/g_next_anc_visit/anc_chosen_date:jr:requiredMsg')" relevant=" /anc_followup/g_next_anc_visit/is_new_anc_scheculed ='yes'"/>
