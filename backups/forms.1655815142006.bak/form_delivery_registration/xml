      <bind nodeset="/delivery_registration/inputs/contact/_id" type="db:person"/>
      <bind nodeset="/delivery_registration/inputs/contact/name" type="string"/>
      <bind nodeset="/delivery_registration/inputs/contact/short_name" type="string"/>
      <bind nodeset="/delivery_registration/inputs/contact/patient_id" type="string"/>
      <bind nodeset="/delivery_registration/inputs/contact/date_of_birth" type="string"/>
      <bind nodeset="/delivery_registration/inputs/contact/sex" type="string"/>
      <bind nodeset="/delivery_registration/inputs/contact/parent/_id" type="string"/>
      <bind nodeset="/delivery_registration/inputs/contact/parent/parent/_id" type="string"/>
      <bind nodeset="/delivery_registration/inputs/contact/parent/parent/parent/_id" type="string"/>
      <bind nodeset="/delivery_registration/inputs/contact/parent/parent/contact/chw_name" type="string"/>
      <bind nodeset="/delivery_registration/inputs/contact/parent/parent/contact/phone" type="string"/>
      <bind nodeset="/delivery_registration/household_id" type="string" calculate="../inputs/contact/parent/_id"/>
      <bind nodeset="/delivery_registration/area_id" type="string" calculate="../inputs/contact/parent/parent/_id"/>
      <bind nodeset="/delivery_registration/facility_id" type="string" calculate="../inputs/contact/parent/parent/parent/_id"/>
      <bind nodeset="/delivery_registration/patient_age_in_years" type="string" calculate="floor( difference-in-months( ../inputs/contact/date_of_birth, today() ) div 12 )"/>
      <bind nodeset="/delivery_registration/patient_uuid" type="string" calculate="../inputs/contact/_id"/>
      <bind nodeset="/delivery_registration/patient_id" type="string" calculate="../inputs/contact/patient_id"/>
      <bind nodeset="/delivery_registration/patient_name" type="string" calculate="../inputs/contact/name"/>
      <bind nodeset="/delivery_registration/has_mother_danger_signs" type="string" calculate="if( /delivery_registration/g_delivery_info/g_mother_danger_signs/mother_danger_sign_bleeding ='yes' or  /delivery_registration/g_delivery_info/g_mother_danger_signs/mother_danger_sign_headaches ='yes' or  /delivery_registration/g_delivery_info/g_mother_danger_signs/mother_danger_sign_abdominal_pain ='yes' or  /delivery_registration/g_delivery_info/g_mother_danger_signs/mother_danger_sign_vomitting ='yes' or  /delivery_registration/g_delivery_info/g_mother_danger_signs/mother_danger_sign_pallor ='yes' or  /delivery_registration/g_delivery_info/g_mother_danger_signs/mother_danger_sign_blurry_vision_loss ='yes' or  /delivery_registration/g_delivery_info/g_mother_danger_signs/mother_danger_sign_breathing ='yes' or  /delivery_registration/g_delivery_info/g_mother_danger_signs/mother_danger_sign_seizures ='yes' or  /delivery_registration/g_delivery_info/g_mother_danger_signs/mother_danger_sign_chest_pain ='yes', 'yes', 'no')"/>
      <bind nodeset="/delivery_registration/m_danger_sign_vaginal" type="string" calculate="if( /delivery_registration/g_delivery_info/g_mother_danger_signs/mother_danger_sign_bleeding ='yes',if( /delivery_registration/inputs/user/language ='en', 'Vaginal spotting/bleeding', 'Taches vaginales/saignements'), '')"/>
      <bind nodeset="/delivery_registration/m_danger_sign_headache" type="string" calculate="if( /delivery_registration/g_delivery_info/g_mother_danger_signs/mother_danger_sign_headaches ='yes',if( /delivery_registration/inputs/user/language ='en', 'severe headaches', 'Maux de tête sévères'), '')"/>
      <bind nodeset="/delivery_registration/m_danger_sign_abdominal" type="string" calculate="if( /delivery_registration/g_delivery_info/g_mother_danger_signs/mother_danger_sign_abdominal_pain ='yes',if( /delivery_registration/inputs/user/language ='en', 'Abdominal pain', 'Douleur abdominale'), '')"/>
      <bind nodeset="/delivery_registration/m_danger_sign_vomiting" type="string" calculate="if( /delivery_registration/g_delivery_info/g_mother_danger_signs/mother_danger_sign_vomitting ='yes',if( /delivery_registration/inputs/user/language ='en', 'Vomiting', 'Vomissement'), '')"/>
      <bind nodeset="/delivery_registration/m_mother_danger_sign_pallor" type="string" calculate="if( /delivery_registration/g_delivery_info/g_mother_danger_signs/mother_danger_sign_pallor ='yes',if( /delivery_registration/inputs/user/language ='en', 'Paleness or pallor', 'Pâleur ou pâleur'), '')"/>
      <bind nodeset="/delivery_registration/m_danger_sign_blurry" type="string" calculate="if( /delivery_registration/g_delivery_info/g_mother_danger_signs/mother_danger_sign_blurry_vision_loss ='yes',if( /delivery_registration/inputs/user/language ='en', 'Blurry or loss of vision', 'Flou ou perte de vision'), '')"/>
      <bind nodeset="/delivery_registration/m_mother_danger_sign_breathing" type="string" calculate="if( /delivery_registration/g_delivery_info/g_mother_danger_signs/mother_danger_sign_breathing ='yes',if( /delivery_registration/inputs/user/language ='en', 'Difficulty breathing', 'Difficulté à respirer'), '')"/>
      <bind nodeset="/delivery_registration/m_mother_danger_sign_seizures" type="string" calculate="if( /delivery_registration/g_delivery_info/g_mother_danger_signs/mother_danger_sign_seizures ='yes',if( /delivery_registration/inputs/user/language ='en', 'Seizures/loss of consciousness', 'Convulsions/perte de conscience'), '')"/>
      <bind nodeset="/delivery_registration/m_mother_danger_sign_chest_pain" type="string" calculate="if( /delivery_registration/g_delivery_info/g_mother_danger_signs/mother_danger_sign_chest_pain ='yes',if( /delivery_registration/inputs/user/language ='en', 'Chest pain', 'Douleur thoracique'), '')"/>
      <bind nodeset="/delivery_registration/m_danger_signs" type="string" calculate="if( /delivery_registration/has_mother_danger_signs ='yes',
concat( /delivery_registration/m_danger_sign_vaginal ,
concat(if( /delivery_registration/m_danger_sign_vaginal !='', ',',''),  /delivery_registration/m_danger_sign_abdominal ),
concat(if( /delivery_registration/m_danger_sign_abdominal !='', ',',''),  /delivery_registration/m_danger_sign_vomiting ),
concat(if( /delivery_registration/m_danger_sign_vomiting !='', ',',''),  /delivery_registration/m_mother_danger_sign_pallor ),
